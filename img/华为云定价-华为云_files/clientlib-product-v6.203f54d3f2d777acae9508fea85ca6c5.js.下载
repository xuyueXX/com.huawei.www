function translation(e, t){
    if( winWidth>1279 && winWidth < 1780){
        $("."+e).children(".product-v6-right-icon").show().animate({right:"-148px",}, t);
    } else if(winWidth>1023 && winWidth < 1279){
        $("."+e).children(".product-v6-right-icon").show().animate({right:"-72px",}, t);
    }else if(winWidth<=1023){
        $(".product-v6-right-icon").hide();
    }else {
        $("."+e).children(".product-v6-right-icon").show().animate({right:"0px",}, t);
    }
}

//淡入效果排他
function fadeExclu(order_index){
    var slideCardLength = $(".product-v6-case-1 .swiper-slide").length;
    for(var i=0;i<=slideCardLength;i++){
        if(i!=order_index){
            $(".product-v6-case-1 .swiper-slide").children(".case-trans-fade").removeClass("case-trans-fadeInUp-degrade");
        }
    }
}



var swiperCase1 = new Swiper('.swiper-case-1', {
    slidesPerView: 'auto',
    autoplay : 5000,
    effect : 'fade',
    roundLengths : true,
    onSlideChangeStart: function(swiper){
        $(".pagination-v6 > .thumbs-btn").removeClass("active");
        $($(".pagination-v6 > .thumbs-btn")[swiper.activeIndex]).addClass("active");
        $(".product-v6-right-icon").hide();
        fadeExclu(swiper.activeIndex);
        translation("swiper-slide-"+swiper.activeIndex, 600);
        $(".swiper-slide-"+swiper.activeIndex).children(".case-trans-fade").addClass("case-trans-fadeInUp-degrade");

        setTimeout(function(){ $(".product-v6-right-icon").css("right","-400px"); }, 100);
    },
});

$(".pagination-v6 > .thumbs-btn").click(function(e){
    e.preventDefault();
    $(this).addClass("active").siblings().removeClass("active");
    var index = $(this).attr("data-index");
    if(winWidth<=768){
        swiperCase1.slideTo(index, 0, false);
    }else{
        swiperCase1.slideTo(index, 600, false);
    }
    $(".product-v6-right-icon").hide();

    fadeExclu(index);
    translation("swiper-slide-"+index, 600);
    $(".swiper-slide-"+index).children(".case-trans-fade").removeClass("case-trans-fadeOutUp-degrade").addClass("case-trans-fadeInUp-degrade");
    $(".product-v6-right-icon").css("right","-400px");
});

