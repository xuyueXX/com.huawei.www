        var src_pc = $this.attr('src-pc');
        var src_bg = (win_width >= 768) ? src_pc : src_mb;

        $this.css('background-image', 'url(' + src_bg +')');
        
    });
}

$(".product-v6-hottopic .islogin-btn").click(function(){
    var url = window.location.href;
    var isIntl = url.indexOf("//intl.");
    if(!window.isLogin) {
        if(isIntl == -1){
            window.location.href = "https://auth.huaweicloud.com/authui/login?service=" + url;
            //window.open("https://auth.huaweicloud.com/authui/login?service=" + url);
            return false;
        }
        else{
            window.location.href = "https://auth-intl.huaweicloud.com/authui/login?service=" + url;
            //window.open("https://auth-intl.huaweicloud.com/authui/login?service=" + url);
            return false;
        }
    } 
});

