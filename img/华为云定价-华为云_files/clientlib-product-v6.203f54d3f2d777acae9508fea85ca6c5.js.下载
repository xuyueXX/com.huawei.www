/***/ "./components/product-v6/components/product-v6-scene1/product-v6-scene1.js":
/*!*********************************************************************************!*\
  !*** ./components/product-v6/components/product-v6-scene1/product-v6-scene1.js ***!
  \*********************************************************************************/
/*! no static exports found */
/***/ (function(module, exports, __webpack_require__) {

/* eslint-disable */
var FlexHeight = __webpack_require__(/*! ./../common/flexHeight */ "./components/product-v6/components/common/flexHeight.js");
$(function(){
    var win_width = window.innerWidth || document.documentElement.clientWidth || document.body.clientWidth;
    $('.product-v6-scene1').each(function () {
        // 全屏看图
        var _this = $(this);
        $(_this).on('click', '.content-img', function () {
            var src = $(this).children('img').attr('src');
            $('body').append('<div class="screenImg"><img src="' + src + '"></div>');
        });
        $(document).on('click', '.screenImg', function () {
            $(this).remove();
        });

        _this.find(".scene-tab").each(function (index, element) {
            $(element).on('click', function() {
                var $con = _this.find(".scene-content")
                    ,target = $($con[index]);
                $(this).addClass('active').siblings().removeClass('active');
                $con.removeClass('active').hide();
                target.addClass('active').show();
                borderFlex();
                minHeight();
                target.find(".config-tab li").each(function(index, element) {
                    $(element).on('click', function() {
                        var $configCon = $(this).parent().siblings('.config-content')
                            ,target = $($configCon[index]);
                        $(this).addClass('active').siblings().removeClass('active');
                        $configCon.removeClass('active');
                        target.addClass('active');
                        minHeight();
                        borderFlex();
                    });
                });
            });
        })

        _this.find(".config-tab li").each(function(index, element) {
            $(element).on('click', function() {
                var $configCon = $(this).parent().siblings('.config-content')
                    ,target = $($configCon[index]);
                $(this).addClass('active').siblings().removeClass('active');
                $configCon.removeClass('active');
                target.addClass('active');
                minHeight();
                borderFlex();
            });
        });

        function borderFlex() {
            if (win_width >= 1200) {
                $('.p-info').each(function(index, element) {
                    new FlexHeight($('.p-info').find('.p-parameter'));
                });
                $('.p-info').each(function(index, element) {
                    new FlexHeight($(element).find('li'));
                });
            }
        }
        borderFlex();

        _this.on('click', '.scene-mb-title', function() {
            $(this).closest('.scene-content').toggleClass('fold');
            $(this).next(".scene-body-layer").slideToggle();
            $(this).closest('.scene-content').siblings('.scene-content').removeClass("fold");
            $(this).closest('.scene-content').siblings('.scene-content').find(".scene-body-layer").slideUp();
        });

        function minHeight() {
            if (innerWidth >= 1200) {
