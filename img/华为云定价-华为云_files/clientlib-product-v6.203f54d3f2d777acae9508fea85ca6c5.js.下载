/***/ "./components/service/components/tab/tab.js":
/*!**************************************************!*\
  !*** ./components/service/components/tab/tab.js ***!
  \**************************************************/
/*! no static exports found */
/***/ (function(module, exports) {

$.fn.setCardHeight=function(el,rowNum){
    $(this).each(function(){
        var heightArr=[];
        var itemNum=$(this).find(el).size();
        for(var i=0; i< itemNum; i++){
            var heightArrIndex1=parseInt(i/rowNum);
            if(heightArr[heightArrIndex1]){
                if(heightArr[heightArrIndex1]<$(this).find(el).eq(i).outerHeight()){
                    heightArr[heightArrIndex1]=$(this).find(el).eq(i).outerHeight();
                }
            }
            else{
                heightArr[heightArrIndex1]=$(this).find(el).eq(i).outerHeight();
            }
        }
        for(var j=0; j< itemNum; j++){
            var heightArrIndex2=parseInt(j/rowNum);
            var maxHeight = heightArr[heightArrIndex2];
            var baseBtn =$(this).find(el).eq(j).find(".case-btn");
            
            if(baseBtn.length>0){
                if(baseBtn.attr("data-click")!="true"){
                    baseBtn.attr("data-click","true");
                    
                    if(baseBtn.parents(".case-btn-hide").length){
                        maxHeight = heightArr[heightArrIndex2];                        
                    }else{
                         maxHeight = heightArr[heightArrIndex2]-60;                         
                    }
                }
                
            }
            $(this).find(el).eq(j).outerHeight(maxHeight);               
                
        }
    });
};


