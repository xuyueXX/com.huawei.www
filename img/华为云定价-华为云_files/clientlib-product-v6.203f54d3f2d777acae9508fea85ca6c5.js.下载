/***/ "./components/product-v6/components/product-v6-advantage/product-v6-advantage.js":
/*!***************************************************************************************!*\
  !*** ./components/product-v6/components/product-v6-advantage/product-v6-advantage.js ***!
  \***************************************************************************************/
/*! no static exports found */
/***/ (function(module, exports) {

/** write js code here */
var win_width = window.innerWidth || document.documentElement.clientWidth || document.body.clientWidth;
if (win_width > 769) {

    if ($('.product-v6-advantage .product-tab.advDropdown').length > 0) {
        $(".product-v6-advantage .product-tab.advDropdown").click(function () {
            var self = $(this),
                $con = self.closest(".product-layer"),
                tar = self.data("target"),
                index = self.index() + 1,
                width = self.outerWidth();

            if (self.hasClass('active')) {
                self.removeClass('active');
                $con.find(".product-v6-content-container").css("height", "0");
                $("." + tar).removeClass('active').fadeOut();
                return;
            }

            self.addClass("active").siblings(".product-tab").removeClass("active");
            var H = $("." + tar).outerHeight(),
                prodLyaer = $con.siblings(".product-layer");
            $con.find(".product-v6-content-container").css("height", H);
            prodLyaer.removeClass("active").find(".product-v6-content").removeClass("active").hide();
            prodLyaer.find(".product-tab").removeClass("active");
            prodLyaer.find(".product-v6-content-container").css("height", "0");

            $con.addClass("active").find(".product-v6-content").removeClass("active").hide();
            $("." + tar).addClass("active").fadeIn();

            $con.find(".indicator-triangle").css({
                left: (index - 1) * width + 84,
            });
        });
    }

}else{

    $('.product-v6-advantage  .product-tab.advDropdown').on("click",function(){
        var $this = $(this);
        var target = $this.attr('data-target');
        var $target = $("." + target);
        if ($("." + target + " .product-v6-adv-body").text().trim() == ""){
            return;}

        if($this.next("." + target).length < 1){
            $this.after($target);
        }
        $('.product-v6-advantage').find('.product-v6-content').not("." + target).slideUp();

        $target.slideToggle();
        $(this).toggleClass('unfold').siblings('.product-tab').removeClass('unfold');
        $(this).closest('.product-layer ').siblings('.product-layer').find('.product-tab').removeClass('unfold');
    });


}




