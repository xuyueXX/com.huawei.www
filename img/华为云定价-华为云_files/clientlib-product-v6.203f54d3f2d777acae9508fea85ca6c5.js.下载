if (win_width <= 768) {
    
    var lang = $("html").attr("lang");

    if(lang == "zh-CN") {
        $(".product-v6-hottopic .common-type-mb").find(".more-btn").find("span").text("展开详情");
    } 
    if(lang == "en-US") {
        $(".product-v6-hottopic .common-type-mb").find(".more-btn").find("span").text("Show More");
    } 

    $(".product-v6-hottopic .common-type-mb").find(".more-btn").on("click", function(){
        var $parents = $(this).parents(".product-v6-hottopic .common-type-mb");
        if($(this).hasClass("active")){
            $(this).removeClass("active");      
            if(lang == "zh-CN") {
                $(this).find("span").text("展开详情");
            } 
            if(lang == "en-US") {
                $(this).find("span").text("Show More");
            } 

            $parents.find(".content-box .info").css("max-height", "66px");
            $parents.find(".hot-news-sub, .hot-r-btn, .hot-r-down").hide();
        }else{
            $(this).addClass("active");
            if(lang == "zh-CN") {
                $(this).find("span").text("收起");
            } 
            if(lang == "en-US") {
                $(this).find("span").text("Show Less");
            } 

            $parents.find(".content-box .info").css("max-height", "unset");
            $parents.find(".hot-news-sub, .hot-r-btn, .hot-r-down").show();
        }
    });

    $(".product-v6-hottopic .common-type-mb").each(function(){

        if($(this).find(".hot-news").height() === 0 || !($(this).find(".hot-news").length)){
            $(this).find(".hot-news-sub, .hot-r-btn, .hot-r-down").show();
            $(this).find(".mb-more").hide();
        }
    });
   
      
