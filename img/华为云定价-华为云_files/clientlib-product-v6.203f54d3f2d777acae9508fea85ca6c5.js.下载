/***/ "./base/ui/js/collapse.js":
/*!********************************!*\
  !*** ./base/ui/js/collapse.js ***!
  \********************************/
/*! no static exports found */
/***/ (function(module, exports, __webpack_require__) {

"use strict";


var util = __webpack_require__(/*! ./util */ "./base/ui/js/util.js");

// 构造函数
var Collapse = function (element) {
    var that = this;
    this.isTransitioning = false;
    this.$element = $(element);

    this.$triggers = $('[data-toggle="v6-collapse"]').filter(function () {
        var $this = $(this);
        var $target_default = $this.closest('.v6-collapse-container').children('.v6-collapse');

        if ($target_default.length > 0 && $target_default.get(0) === that.$element.get(0)) {
            return true;
        }

        var targetSelector = $this.attr('data-target') || $this.attr('href');

        if ($(targetSelector).toArray().indexOf(that.$element.get(0)) !== -1) {
            return true;
        }
    });
};

Collapse.prototype.show = function () {
    var that = this;

    if (this.isTransitioning ||
        this.$element.hasClass('show')) {
        return;
    }

    this.isTransitioning = true;

    this.$triggers.addClass('expended');

    this.$element
        .trigger('beforeShow')
        .removeClass('v6-collapse')
        .addClass('collapsing');

    var contentHeight = this.$element.get(0).scrollHeight;
    var transitionDuration = util.getTransitionDurationFromElement(this.$element.get(0));

    this.$element
        .height(contentHeight)
        .trigger('beginShow')
        .one('transitionend', function () {
            that.isTransitioning = false;
            $(this)
                .removeClass('collapsing')
                .addClass('v6-collapse')
                .addClass('show')
                .height('')
                .trigger('shown');

        })
        .transitionEndEmulator(transitionDuration)
        .trigger('afterShow');
};

Collapse.prototype.hide = function () {
    var that = this;

    if (this.isTransitioning ||
        !this.$element.hasClass('show')) {
        return;
    }

    this.isTransitioning = true;

    this.$triggers.removeClass('expended');

    var contentHeight = this.$element.height();
    this.$element
        .trigger('beforeHide')
        .height(contentHeight)
        .removeClass('v6-collapse')
        .addClass('collapsing')
        .trigger('beginHide');

    var transitionDuration = util.getTransitionDurationFromElement(this.$element.get(0));

    this.$element
        .one('transitionend', function () {
            that.isTransitioning = false;
            $(this)
                .removeClass('show collapsing')
                .addClass('v6-collapse')
                .trigger('hidden');
        })
        .transitionEndEmulator(transitionDuration)
        .height('');
};

Collapse.prototype.toggle = function () {
    if (this.$element.hasClass('show')) {
        this.hide();
    } else {
        this.show();
    }
};

// 插件
$.fn.v6collapse = function (method) {
    return this.each(function () {
        var $this = $(this);
        var instance = $this.data('vui.collapse');

        if (!instance) $this.data('vui.collapse', (instance = new Collapse(this)));
        if (typeof method === 'string') instance[method]();
    });
};

// 生成实例
$(document).on('click.vui.collapse', '[data-toggle="v6-collapse"]', function (e) {
    e.preventDefault();
    var $this = $(this);
    var targetSelector = $this.attr('data-target') || $this.attr('href');
    var $target = targetSelector ? $(targetSelector) : $this.closest('.v6-collapse-container').children('.v6-collapse');

    $target.v6collapse('toggle');

    if ($target.hasClass('show')) return;

    var $closeSiblings = $this.closest('.v6-collapse-container[data-hide-siblings]');

    if ($closeSiblings.length > 0) {
        $closeSiblings
            .siblings('.v6-collapse-container')
            .children('.v6-collapse.show')
            .v6collapse('hide');
    }
});

/***/ }),

/***/ "./base/ui/js/select.js":
/*!******************************!*\
  !*** ./base/ui/js/select.js ***!
  \******************************/
/*! no static exports found */
/***/ (function(module, exports) {

!function ($) {
    var Select = function (element, options) {
        this.$element = $(element);
        this.options = options;
        this._init();
    };

    Select.prototype._init = function () {
        var that = this;
        var listItemsStr = '';
        var selectedText = this.$element.find('option:checked').html();

        this.$element.find('option').each(function (index) {
            var $option = $(this);
            var text = $option.html();
            var selected = $option.is(":checked");
            var classValue = $.trim((selected ? 'selected ' : '') + ($option.attr('class') || ''));
            var attrClass = classValue ? 'class="' + classValue + '"' : '';
            var attrs = '';

            $.each(this.attributes, function () {
                if(this.name === 'class' || this.name === 'data-index') return true;
                attrs += (this.name + '="' + this.value + '"');
            });

            attrs = $.trim(attrClass + ' ' + attrs);

            listItemsStr += '<li ' + attrs + ' data-index="' + index + '">' + text + '</li>';
        });

        this.$element
            .append('<div class="v6-select-trigger"><span>' + selectedText + '</span></div>')
            .append('<ul class="v6-select-option-list">' + listItemsStr + '</ul>');

        this.$element.on('click', '.v6-select-trigger', function () {
            that.toggle();
        });

        this.$element.on('click', '.v6-select-option-list > li', function () {
            that.choice(this);
        });

        $(document).on('click', function (e) {
            var $target = $(e.target);

            if ($target.closest('.v6-select').get(0) !== that.$element.get(0)) that.close();
        });
    };

    Select.prototype.open = function () {
        this.$element.addClass('open');
    };

    Select.prototype.close = function () {
        this.$element.removeClass('hover open');
    };

    Select.prototype.toggle = function (state) {
        this.$element.toggleClass('open', state);
    };

    Select.prototype.choice = function (option) {
        var $option = $(option);
        var selectedIndex = $option.attr('data-index');
        var $select = this.$element.find('select');
        var $triggerInner = this.$element.find('.v6-select-trigger span');

        this.close();

        if ($option.hasClass('selected')) return;

        $option.addClass('selected').siblings().removeClass('selected');
        $select.children().get(selectedIndex).selected = true;
        $triggerInner.text($select.children(':checked').html());
        $select.trigger('change');
    };

    var plugin = function (method, options) {
        return this.each(function () {
            var $this = $(this);
            var instance = $this.data('vui.select');

            if (!instance) $this.data('vui.select', instance = new Select(this));

            if (typeof method === 'string') instance[method](options);
        });
    };

    $.fn.v6select = plugin;
    $.fn.v6select.Constructor = Select;

    $(document)
        .on('mouseenter.vui.select', '.v6-select', function () {
            $(this).addClass('hover');
        })
        .on('mouseleave.vui.select', '.v6-select', function () {
            $(this).removeClass('hover');
        });

    $(document).on('mouseenter.vui.select', '.v6-select', function () {
        var $this = $(this);
        var instance = $this.data('vui.select');

        if (instance) return;

        $this.data('vui.select', plugin.call($this));
    });

    $('.v6-select').each(function(){
        var $this=$(this);
        var initialValue=$this.find('select option:eq(0)').text();
        $this.append('<div class="v6-select-trigger"><span>'+initialValue+'</span></div>');
    });

    $.fn.v6selectReader = function(selectItem, optionIndex){
        return this.each(function () {
            var $this = $(this);
            var selectOptionsHtml='';
            var viewOptionsHtml='';
            var $thisSelect=$this.find('select');
            var $thisView=$this.find('ul.v6-select-option-list');
            var viewValueDom=$this.find(".v6-select-trigger span");
            for(var i = 0; i < selectItem.length; i++){
                selectOptionsHtml+='<option value="'+selectItem[i]+'">'+selectItem[i]+'</option>';
                viewOptionsHtml  +=(i===optionIndex)?'<li class="selected" data-index="'+i+'">'+selectItem[i]+'</li>':'<li data-index="'+i+'">'+selectItem[i]+'</li>';
            }
            $thisSelect.html(selectOptionsHtml);
            $thisView.html(viewOptionsHtml);
            $thisSelect.val(selectItem[optionIndex]);
            viewValueDom.html(selectItem[optionIndex]);
        });
    };
}(jQuery);

/***/ }),

/***/ "./base/ui/js/tab.js":
/*!***************************!*\
  !*** ./base/ui/js/tab.js ***!
