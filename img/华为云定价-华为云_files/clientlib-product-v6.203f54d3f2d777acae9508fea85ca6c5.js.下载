/***/ "./components/product-v6/components/product-v6-instance/product-v6-instance.js":
/*!*************************************************************************************!*\
  !*** ./components/product-v6/components/product-v6-instance/product-v6-instance.js ***!
  \*************************************************************************************/
/*! no static exports found */
/***/ (function(module, exports, __webpack_require__) {

var FlexHeight = __webpack_require__(/*! ./../../../../util/flexHeight */ "./util/flexHeight.js");


// 入门推荐
$('.v6-section-instance').each(function () {
    var $this = $(this);
    var $configs = $this.find('.v6-instance-recommend .v6-instance-recommend-config');
    var flex = new FlexHeight($configs);

    $configs
        .closest('.v6-tab-content')
        .on('afterShow', function () {
            flex.equal();
        });
});

// 实例总览
$('.v6-instance-all').each(function () {
    var $this = $(this);

    // region 过滤
    !function () {
        var $filterRadio = $this.find('.v6-filter-btn input');
        var $instanceItems = $this.find('.v6-instance-list-inner');

        $filterRadio.on('change', function () {
            var val = $(this).val();

            $instanceItems
                .removeClass('show')
                .filter('[data-type="' + val + '"]')
                .addClass('show');
        });
    }();
    // endregion

    // region 展开和收起
    !function () {
        $this.on('click', '.v6-instance-card', function () {
            var $this = $(this);

            if ($this.hasClass('open')) return;

            $this
                .closest('.v6-instance-list')
                .find('.v6-instance-list-inner.show .v6-instance-card')
                .removeClass('open');

            $this.addClass('open').trigger('afterOpen');
        });
    }();
    // endregion

    // region 推荐配置帮助tip
    !function () {
        var $configs = $this.find('.v6-instance-card-config-recommend');
        $configs.each(function () {
            var $config = $(this);
            var $tip = $config.find('.v6-instance-card-config-recommend-help-tip');

            $config.on('mouseenter mouseleave', '.v6-instance-card-config-recommend-text .u-icon,.v6-instance-card-config-recommend-text .v6-instance-card-config-recommend-help-tip', function (e) {
                $(this).toggleClass('hover', e.type === 'mouseenter');
            });

            $config.on('mouseenter', '.v6-instance-card-config-recommend-text .u-icon', function () {
                var $icon = $(this);
                var $tipArrow = $tip.find('i');
                var position = $icon.position();
                var top = position.top + $icon.outerHeight();
                var left = position.left + $icon.width() / 2 + parseInt($icon.css('margin-left'));

                $tip.css({
                    'top': top,
                });
                $tipArrow.css({
                    'left': left,
                });

                $tip.addClass('show');
            });

            $config.on('mouseleave', '.v6-instance-card-config-recommend-text .u-icon,.v6-instance-card-config-recommend-text .v6-instance-card-config-recommend-help-tip', function () {
                var $tip = $config.find('.v6-instance-card-config-recommend-help-tip');

                setTimeout(function () {
                    if ($config.find('.v6-instance-card-config-recommend-text .u-icon,.v6-instance-card-config-recommend-text .v6-instance-card-config-recommend-help-tip').filter('.hover').length === 0) {
                        $tip.removeClass('show');
                    }
                }, 0);
            });
        });
    }();
    // endregion

    // region 配置选择
    !function () {
        var $configs = $this.find('.v6-instance-card-config-recommend');
        $configs.each(function () {
            var $config = $(this);
            var $selects = $config.find('select');
            var $targets = $config.find('.v6-instance-card-config-recommend-list,.v6-instance-card-config-recommend-buy');

            $config.on('change', 'select', function () {
                var choice = '';

                $selects.each(function (index) {
                    choice += (index > 0 ? '.' : '') + $(this).val();
                });

                $targets
                    .removeClass('active')
                    .filter('[data-name="' + choice + '"]')
                    .addClass('active');
            });
        });
    }();
    // endregion

    //region 查询价格
    !function () {
        var $instanceCard = $this.find('.v6-instance-card');
        var priceApi = $this.attr('data-price-api');
        var queryPlaceholder = $this.attr('data-query-placeholder');
        var priceSuffix = $this.attr('data-price-suffix');
        var $filterRadio = $this.find('.v6-filter-btn input');
        var isTest = $this.attr('data-price-api-test');

        var queryPrice = function (regionID, config, success) {
            if (isTest) {
                console.log('test query config: ' + config);
                setTimeout(function () {
                    success({amount: 7654321, currency: 'CNY',});
                    console.log('success test query config: ' + config);
                }, 700);
            } else {
                $.ajax({
                    url: priceApi,
                    type: 'post',
                    data: JSON.stringify({
                        "regionId": regionID,
                        "chargingMode": 0,
                        "periodType": 2,
                        "periodNum": 1,
                        "subscriptionNum": 1,
                        "productInfos": [{
                            "id": "10",
                            "cloudServiceType": "hws.service.type.ec2",
                            "resourceType": "hws.resource.type.vm",
                            "resourceSpecCode": config,
                            "resourceSize": "",
                            "resouceSizeMeasureId": "",
                            "usageFactor": "Duration",
                            "usageValue": 1,
                            "usageMeasureId": "",
                        },],
                    }),
                    dataType: 'json',
                    success: success,
                });
            }
        };
        var renderPrice = function ($el, price, currency) {
            var pricePrefixMap = {
                'USD': '$',
                'CNY': '￥',
            };
            /**
             * 格式化价格（加千分符，固定小数点位数）
             * @param number 价格
             * @param length 小数点位数，默认值为2
             */
            var priceConverter = function (number, length) {
                number = number + '';
                length = length === void 0 ? 2 : length;
                // 处理整数部分
                var integer = number.split('.')[0];
                var integerSlices = integer.split('');
                var i;

                for (i = integerSlices.length - 4; i >= 0; i = i - 3) {
                    integerSlices[i] = integerSlices[i] + ','
                }

                integer = integerSlices.join('');

                // 处理小数部分
                var decimal = number.split('.')[1] || '';
                var decimalLength = decimal.length;

                if(decimalLength > length) {
                    decimal = decimal.substring(0,length);
                }

                if(decimalLength < length) {
                    for(i = 0; i < length - decimalLength; i++){
                        decimal += '0';
                    }
                }

                return integer + '.' + decimal;
            };

            $el.find('.v6-instance-card-config-recommend-price:visible').html('<em>' + (pricePrefixMap[currency.toUpperCase()] || '') + priceConverter(price) + (currency === 'USD' ? ' USD' : '') + '</em>' + priceSuffix).addClass('init');
        };
        var getPrice = function ($config) {
            var regionID = $config.attr('data-region-id') || 'cn-north-4';
            var $price = $config.find('.v6-instance-card-config-recommend-price:visible');

            if ($price.hasClass('init')) return;

            var $selects = $config.find('select');

            if ($selects.length === 0) return;

            var isConfigSys = $selects.length > 1;
            var choice = '';

            $selects.each(function (index) {
                choice += (index > 0 ? '.' : '') + $(this).val();
            });

            if (!isConfigSys) {
                choice += '.linux';
            }

            $price.html('<em>' + queryPlaceholder + '</em>');

            queryPrice(regionID, choice, function (data) {
                renderPrice($config, data.amount, data.currency);
            });
        };

        // 初始化显示的配置价格
        $instanceCard.find('.v6-instance-card-config-recommend:visible').each(function () {
            getPrice($(this));
        });

        // 选择配置后更新价格
        $instanceCard.on('change', 'select', function () {
            getPrice($(this).closest('.v6-instance-card-config-recommend'));
        });

        // 展开卡片后更新价格
        $instanceCard.on('afterOpen', function () {
            getPrice($(this).find('.v6-instance-card-config-recommend'));
        });

        // 切换过滤后更新价格
        $filterRadio.on('change', function () {
            $instanceCard.find('.v6-instance-card-config-recommend:visible').each(function () {
                getPrice($(this));
            });
        });
    }();
    //endregion
});

