/***/ "./util/flexHeight.js":
/*!****************************!*\
  !*** ./util/flexHeight.js ***!
  \****************************/
/*! no static exports found */
/***/ (function(module, exports) {

function FlexHeight($items) {
    var that = this;
    this.autoEqual = true;
    this.$items = $items;
    this.equal();
    if ($.throttle) {
        $(window).resize($.throttle(function () {
            that.equal();
        }));
    } else {
        $(window).resize(function () {
            if (!that.autoEqual) return;
            that.equal();
        });
    }
}

FlexHeight.prototype.equal = function () {
    var maxHeight = 0;
    this.$items
        .height('')
        .each(function () {
            var height = $(this).outerHeight();
            if (maxHeight < height) {
                maxHeight = height;
            }
        })
        .outerHeight(maxHeight);
};

FlexHeight.prototype.auto = function (param) {
    if (param === undefined || this.autoEqual === param) {
        return;
    }

    this.autoEqual = !!param;

    if (this.autoEqual) {
        this.equal();
    } else {
        this.$items.height('');
    }

};

module.exports = FlexHeight;

/***/ }),

/***/ "./util/jquery.flexHeight.js":
/*!***********************************!*\
  !*** ./util/jquery.flexHeight.js ***!
  \***********************************/
/*! no static exports found */
/***/ (function(module, exports) {

/* 组件：弹性高度盒子
 * 依赖：$.throttle
 =================*/
!function ($) {
    'use strict';

    // FLEX CLASS DEFINITION
    // =====================

    function FlexHeight($items) {
        var that = this;
        this.autoEqual = true;
        this.$items = $items;
        this.equal();
        if($.throttle) {
            $(window).resize($.throttle(function () {
                that.equal();
            }));
        } else {
            $(window).resize(function () {
                that.equal();
            });
        }
    }

    FlexHeight.prototype.equal = function () {
        if(!this.autoEqual) return;
        var maxHeight = 0;
        this.$items
            .height('')
            .each(function () {
                var height = $(this).height();
                if (maxHeight < height) {
                    maxHeight = height;
                }
            })
            .height(maxHeight);
    };

    FlexHeight.prototype.auto = function (param) {
        if(param === undefined || this.autoEqual == param) {
            return;
        }

        this.autoEqual = !!param;

        if(this.autoEqual) {
            this.equal();
        } else {
            this.$items.height('');
        }

    };

    // FLEX_HEIGHT PLUGIN DEFINITION
    // =============================

    var old = $.fn.flexHeight;

    $.fn.flexHeight = function (option,param) {
        return this.each(function () {
            var $this = $(this);
            var $items;
            var data = $this.data('hw.flexHeight');

            if(!data) {
                if(option && option.items) {
                    $items = $this.find(option.items);
                } else {
                    $items = $this.children('.flex-item');
                }
                $this.data('hw.flexHeight', new FlexHeight($items));
            }

            if(typeof option == 'string') {
                data[option](param);
            }
        });
    };

    $.fn.flexHeight.Constructor = FlexHeight;


    // FLEX_HEIGHT NO CONFLICT
    // =======================

    $.fn.flexHeight.noConflict = function () {
        $.fn.flexHeight = old;
        return this;
    };


    // FLEX_HEIGHT AUTO INIT
    // =====================

    $(document).ready(function () {
        $('[data-toggle="flexHeight"]').flexHeight();
    });

}(jQuery);

/***/ }),

/***/ "./util/mediaQuery.js":
/*!****************************!*\
  !*** ./util/mediaQuery.js ***!
  \****************************/
/*! no static exports found */
/***/ (function(module, exports) {

module.exports = function (width) {
    var time = (new Date).getTime();
    var name = 'mediaQuery' + time;
    var htmlStr = '<span class="'+name+'"><style>.'+name+'{font-size: 0 !important;}@media(min-width: '+width+'px){.'+name+'{font-size: 20px !important;}}</style></span>';
    var $query = $('body').append(htmlStr).find('.' + name);
    var is_gtWidth = ($query.css('font-size') === '20px');

    $query.remove();

    return is_gtWidth;
};

/***/ }),

/***/ "./util/parallaxScroll.js":
/*!********************************!*\
  !*** ./util/parallaxScroll.js ***!
  \********************************/
/*! no static exports found */
/***/ (function(module, exports, __webpack_require__) {

/*
 * 视差滚动插件
 *
 */
!function (global, factory) {
    if(true) {
        module.exports = factory();
    } else {}
}(typeof window !== "undefined" ? window : this, function () {
    var utils = {
        trim: function (string){
            return string.replace(/^[\s]+/, '').replace(/[\s]+$/, '');
        },
        isObject: function (obj) {
            return typeof obj === 'object' && !Array.isArray(obj);
        },
        getTop2Window: function (elem) {
            return elem.getBoundingClientRect().top;
        },
        limitNumber: function (number, min, max) {
            if (number < min) return min;
            if (number > max) return max;
            return number;
        },
        getStyleRenderers: function (string) {
            var styleRenderers = {};

            string.split(';').forEach(function (item) {
                if(!utils.trim(item)) return;

                var itemArr = item.split(':');
                var t = itemArr[1].split(/[0-9.-]+/).join('$');

                var functionBody = 'var times = 0;return \'' + t + '\'.replace(/\\$/g, function (word) {return values[times++];});';

                styleRenderers[utils.trim(itemArr[0])] = new Function ('values', functionBody);
            });

            return styleRenderers;
        },
        getStyleValuesByString: function (string) {
            var styleValues = {};

            string.split(';').forEach(function (item) {
                var itemArr = item.split(':');

                styleValues[utils.trim(itemArr[0])] = string.match(/[0-9.-]+/g).map(function (string) {
                    return parseFloat(string);
                });
            });

            return styleValues;
        },
        getStyleValuesByRatio: function (startStyleValues, endStyleValues, ratio) {
            var styleValues = {};

            for (var property in startStyleValues) {

                styleValues[property] = startStyleValues[property].map(function (item, index) {
                    return (endStyleValues[property][index] - item) * ratio + item;
                });
            }

            return styleValues;
        },
    };

    var ParallaxScroll = function (options) {
        var that = this;

        this.defaults = {
            containerSelector: '.parallaxScroll-container',
            innerSelector: '.parallaxScroll-inner',
            startTop: 0,
            endTop: 0,
            translate3d: true,
        };

        if (!utils.isObject(options)) {
            options = {};
        }

        this.options = $.extend({}, this.defaults, options);

        this.$container = $(this.options.containerSelector);
        this.$inners = $(this.options.innerSelector);
        this.options.endTop = -this.$container.height();
        this.effectiveRange = this.options.endTop - this.options.startTop;

        that.initInners();
        that.setInnersStyle();

        $(window).scroll(function () {
            that.setInnersStyle();
        });
    };

    ParallaxScroll.prototype.initInners = function () {
        this.$inners.each(function () {
            var $inner = $(this);
            var startStyle = $inner.attr('data-start-style');
            var endStyle = $inner.attr('data-end-style');

            $inner.data('__parallaxScroll__', {
                styleRenderers: utils.getStyleRenderers(startStyle),
                startStyleValues: utils.getStyleValuesByString(startStyle),
                endStyleValues: utils.getStyleValuesByString(endStyle),
            });
        });
    };

    ParallaxScroll.prototype.getContainerMoveRatio = function () {
        var top2Window = utils.limitNumber(utils.getTop2Window(this.$container[0]), this.options.endTop, this.options.startTop);

        return (top2Window - this.options.startTop) / this.effectiveRange;
    };


    ParallaxScroll.prototype.setInnersStyle = function () {
        var that = this;
        var containerMoveRatio = that.getContainerMoveRatio();
        that.$inners.each(function () {
            var $inner = $(this);
            var __parallaxScroll__ = $inner.data('__parallaxScroll__');
            var styleRenderers = __parallaxScroll__.styleRenderers;
            var startStyleValues = __parallaxScroll__.startStyleValues;
            var endStyleValues = __parallaxScroll__.endStyleValues;
            var currentValues = utils.getStyleValuesByRatio(startStyleValues, endStyleValues, containerMoveRatio);
            var style = {};

            for(var property in styleRenderers) {

                style[property] = styleRenderers[property](currentValues[property]);
            }

            $inner.css(style);
        });

    };

    return ParallaxScroll;
});

/***/ })

/******/ });
//# sourceMappingURL=index.min.js.map