$(function(){
    var winWidth =
    window.innerWidth ||
    document.documentElement.clientWidth ||
    document.body.clientWidth;
    var $ad = $('.product-ad-mb');
    var adPadding = (winWidth > 767) ? "3.5%" : 15;
    $ad.each(function(){
        var $this = $(this);
        var window_width = window.innerWidth;
        var slideItem = $this.find('.product-ad-list>li');

        if(window_width >= 1024 || slideItem.length <= 1) return;

        var container = $this.children();
        new Swiper(container, {
            pagination: container.find('.product-ad-pagination'),
            //paginationClickable: true,
            spaceBetween: adPadding,
            centeredSlides: true,
            autoHeight: true,  //enable auto height
            autoplay: 3000,
            autoplayDisableOnInteraction: false,
            simulateTouch: false,
            loop : true,
        });
    });

    $(".product-v6-ad .swiper-slide").each(function(){

        var $desc = $(this).find(".ad-desc");
        var titleW = Math.ceil($(this).find(".ad-title").width()) + 10;
        var iconW = Math.ceil($(this).find(".label-icon").width()) + 20;
       
        // var adBoxHeight = $(".product-v6-ad").height();
        // $(".product-v6-ad").css("margin-top", -adBoxHeight);

        
        
        $desc.css("max-width", "calc(100% - " + (titleW + iconW )+ "px)");
        // var w=$desc.width();

        var containerLength = $desc.width();
        var textLength = $desc.get(0).scrollWidth;
        var text = $desc.html();
        if (textLength > containerLength) {
            $desc.attr( "title", text);
        } else {
            $desc.removeAttr( "title");
        }
    });

});

