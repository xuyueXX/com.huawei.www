/***/ "./components/service/components/service-technology-card/service-technology-card.js":
/*!******************************************************************************************!*\
  !*** ./components/service/components/service-technology-card/service-technology-card.js ***!
  \******************************************************************************************/
/*! no static exports found */
/***/ (function(module, exports, __webpack_require__) {

var mediaQuery = __webpack_require__(/*! ./../../../../util/mediaQuery */ "./util/mediaQuery.js");

$(function() {
    var winWidth =
        window.innerWidth ||
        document.documentElement.clientWidth ||
        document.body.clientWidth;

    $('.service-v6-section-technology-card').each(function(){

        var $this = $(this);
        $this.find('[tab-title-list] li').click(function () {

			var oLi = this;
            var thisIndex=$(this).index();
            $(this).addClass('active').siblings().removeClass('active');
            var thisTabody=$(this).closest('.tabs-head').next('.tabs-body');

            if(winWidth > 767){
                thisTabody.find('.cardlist').eq(thisIndex).show(1,function () { }).siblings().hide();
            }else{
                thisTabody.find('.cardlist').eq(thisIndex).show().siblings().hide();
            }

            oLi.timer = setInterval(function() {
                if(oLi.className.indexOf('active') !=-1) {
                    oLi.timer = null;
                    textOverflow($this.find('.tab-title-list'));
                }
            }, 1);
        });

        $this.find('.cardlist').each(function() {
            var $this = $(this);
            var $cards = $this.find('.service-v6-technology-card');
            var $more = $this.find('.service-v6-technology-more');
            var cardLength = $cards.length;
            var isMb = !mediaQuery(767);

            if ((isMb && cardLength > 3) || (!isMb && cardLength > 9)) {
                $more.addClass('show');
            }

            $this.find('.service-v6-technology-more a').on('click', function () {
                var $a = $(this);
                var $wrapper = $this.find('.service-v6-row');
                var willExpend = !$a.hasClass('expended');

                $a.toggleClass('expended', willExpend);
                $wrapper.toggleClass('expended', willExpend);
            });

        });

        var $swiperContainer=$(this).find(".scroll-list-wrapper.swiper-container"),
            $uLeft=$(this).find(".u-icon-left"),
            $uRight=$(this).find(".u-icon-right"),
            $uMbLeft = $(this).find(".u-icon-l"),
            $uMbRight=$(this).find(".u-icon-r");

        new Swiper($swiperContainer,{
            slidesPerView: 'auto',
            prevButton: $uLeft,
            nextButton: $uRight,
            breakpoints: {
                //当宽度小于等于768
                768: {
                    // slidesOffsetBefore : 8,
                    // slidesOffsetAfter : 8,
                    onInit: function(swiper) {
                        if(swiper.slides.length > 4) {
                            $uMbRight.fadeIn();
                        }
                    },
                    onTransitionStart: function(swiper) {
                        if(swiper.slides.length > 4) {
                            $uMbLeft.fadeIn();
                            $uMbRight.fadeIn();
                            if(swiper.isEnd) {
                                $uMbRight.fadeOut();
                            }
                            if(swiper.isBeginning) {
                                $uMbLeft.fadeOut();
                            }
                        }
                    },
                },
            },
            onInit: function(swiper) {
                if((swiper.slides.length == 7 || swiper.slides.length == 6) && winWidth > 1439) {
                    swiper.prevButton.css('display','none');
                    swiper.nextButton.css('display','none');
                }
            },

        });

        textOverflow($this.find('.tab-title-list'));
    });

    function textOverflow($el) {

        if(winWidth > 767){
            var initSize = 18;

            $('li', $el).each(function(_index, ele) {
                if($(ele).outerHeight() < 64) {
                    $(ele).find('span').css({
                        'line-height': '48px',
                    });
                }
            });

            while ($el.outerHeight() > 64 && initSize > 14) {
                initSize -= 2;
                var tmpSize = initSize;
                $('span', $el).each(function(_index, ele) {
                    $(ele).css({
                        'font-size': tmpSize,
                    });
                });
            }

			$('li', $el).each(function(_index, ele) {
                if($(ele).outerHeight() < 64) {
                    $(ele).find('span').css({
                        'line-height': '48px',
                    });
                }
            });

        }else {
            $('li', $el).each(function(_index, ele) {
                if($(ele).outerHeight() < 44) {
                    $(ele).find('span').css({
                        'line-height': '40px',
                    });
                }
            });
        }

        $el.find('li').each(function(_index, ele) {
            var maxH = winWidth > 767 ? 64 : 44;
            var diff = winWidth > 767 ? 48 : 40;
            $(ele).data('myTitle', $(ele).text());
            if($(ele).outerHeight() > maxH) {
                $(ele).height(maxH);
                $(ele).find('span').css({
                    'height': diff,
                    'display': '-webkit-box',
                    '-webkit-box-orient': 'vertical',
                    '-webkit-line-clamp': '2',
                    'overflow': 'hidden',
                });

                $(ele)
                    .mouseover(function(e) {
                        var tooltip = "<div class='service-v6-technology-card-tab-tip'>" + $(ele).data('myTitle') + '</div>';
                        $('body').append(tooltip);
                        $('.service-v6-technology-card-tab-tip')
                            .css({
                                top: e.pageY + 20 + 'px',
                                left: e.pageX + 10 + 'px',
                            })
                            .show('fast');
                    })
                    .mouseout(function() {
                        $('.service-v6-technology-card-tab-tip').remove();
                    })
                    .mousemove(function(e) {
                        $('.service-v6-technology-card-tab-tip').css({
                            top: e.pageY + 20 + 'px',
                            left: e.pageX + 10 + 'px',
                        });
                    });
            }

        });
    }

});



