var itemsWidth = setWidth($(".breadcrumb > li:not(:last)"));
function setWidth(item){
    var itemsWidth = 0;
    item.each(function(){
        itemsWidth += $(this).innerWidth() + 6;
    });
    return itemsWidth;
}
$(".breadcrumb > li:last-child").css("max-width","calc(100% - " + itemsWidth + "px)");


var headerMethods = {};
headerMethods.setTheme = function (strategy) {
    var strategies = {
        'transparent-light': function () {
            $('#header').attr('data-theme', 'transparent-light');
        },
        'default': function () {
            $('#header').attr('data-theme', '');
        },
        'auto': function () {
            var set = function () {
                $('#header').attr('data-theme', $(window).scrollTop() > 0 ? '' : 'transparent-light');
            };

            $(window).scroll(set);
            set();
        },
    };

    strategies[strategy]();
};

if (mediaQuery(1024) && $(".service-banner .product-banner-content").length) {
    headerMethods.setTheme('auto');
}

