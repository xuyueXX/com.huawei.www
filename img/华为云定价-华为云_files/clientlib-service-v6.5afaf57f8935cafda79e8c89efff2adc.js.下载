/***/ "./components/service/components/service-table/service-table.js":
/*!**********************************************************************!*\
  !*** ./components/service/components/service-table/service-table.js ***!
  \**********************************************************************/
/*! no static exports found */
/***/ (function(module, exports, __webpack_require__) {

var FlexHeight = __webpack_require__(/*! ./../../../../util/flexHeight */ "./util/flexHeight.js");
var $table = $('.service-v6-table');

// 按行排布的表格
$table.each(function () {
    var $thisTable = $(this);

    $thisTable.on('mouseenter', '.service-v6-table-row .service-v6-table-cell', function () {
        var $hoverCell = $(this);
        var hoverIndex = $hoverCell.index();

        if (hoverIndex === 0) return;

        $thisTable
            .find('.service-v6-table-cell')
            .removeClass('active');

        $thisTable.find('.service-v6-table-row').each(function () {
            var $row = $(this);

            $row
                .find('.service-v6-table-cell')
                .eq(hoverIndex)
                .addClass('active');
        });
    });

    if ($thisTable.hasClass('service-v6-table-fixed-head')) {
        var $tableHead = $thisTable.find('.service-v6-table-head');
        var $tableHeadRow = $tableHead.find('.service-v6-table-row');
        var $tableLastRow = $thisTable.find('.service-v6-table-row').last();

        $(window).scroll(function () {
            var $pagenav = $('.pagenav-box');
            var headerHeight = $('#header').outerHeight();
            var tableTop = $thisTable.get(0).getBoundingClientRect().top;
            var lastRowTop = $tableLastRow.get(0).getBoundingClientRect().top;
            var fixedTop = $pagenav.length ? $pagenav.get(0).getBoundingClientRect().top + $pagenav.outerHeight() : 0;
            var tableTop2Header = tableTop - fixedTop;
            var lastRowTop2Header = lastRowTop - fixedTop;
            var tableHeadHeight = $tableHead.outerHeight();
            var lastRowTop2tableHeader = lastRowTop2Header - tableHeadHeight;

            if (tableTop2Header < 0 && lastRowTop2Header > 0) {
                if (lastRowTop2tableHeader < 0) {
                    fixedTop = fixedTop + lastRowTop2tableHeader;
                }

                $tableHead.height($tableHeadRow.height());
                $tableHeadRow
                    .addClass('v6-container')
                    .css({
                        position: 'fixed',
                        top: fixedTop,
                        width: '100%',
                    });
            } else {
                $tableHead.height('');
                $tableHeadRow
                    .removeClass('v6-container')
                    .css({
                        position: '',
                        top: '',
                        width: '',
                    });
            }
        });
    }
});

// 按列排布的表格
$table.each(function () {
    var $thisTable = $(this);

    $thisTable
        .find('.service-v6-table-column')
        .eq(0)
        .find('.service-v6-table-head-cell,.service-v6-table-cell')
        .each(function (index) {
            var $this = $(this);
            var $thisCol = $this.closest('.service-v6-table-column');
            var flexItems = $this;

            $thisCol.siblings().each(function () {
                flexItems = flexItems.add($(this).find('.service-v6-table-head-cell,.service-v6-table-cell').eq(index));
            });

            new FlexHeight(flexItems);
        });

    $thisTable.on('mouseenter', '.service-v6-table-column', function () {
        var $hoverCol = $(this);
        var hoverColIndex = $hoverCol.index();

        if (hoverColIndex === 0) return;

        $hoverCol
            .addClass('active')
            .siblings()
            .removeClass('active');
    });
});

