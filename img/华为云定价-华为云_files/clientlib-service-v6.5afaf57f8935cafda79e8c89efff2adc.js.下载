/***/ (function(module, exports) {

$.fn.equalHeight=function(el,rowNum){
    $(this).each(function(){
        var heightArr=[];
        var itemNum=$(this).find(el).size();
        for(var i=0; i< itemNum; i++){
            var heightArrIndex1=parseInt(i/rowNum);
            if(heightArr[heightArrIndex1]){
                if(heightArr[heightArrIndex1]<$(this).find(el).eq(i).outerHeight()){
                    heightArr[heightArrIndex1]=$(this).find(el).eq(i).outerHeight();
                }
            }
            else{
                heightArr[heightArrIndex1]=$(this).find(el).eq(i).outerHeight();
            }
        }
        for(var j=0; j< itemNum; j++){
            var heightArrIndex2=parseInt(j/rowNum);
            $(this).find(el).eq(j).outerHeight(heightArr[heightArrIndex2]);
        }
    });
};

var winWidth =
        window.innerWidth ||
        document.documentElement.clientWidth ||
        document.body.clientWidth;
var setflexHeight= function (){
    if(winWidth > 1023){
        $('.service-v6-tabvideo').each(function(){      
            var thisCardlist=$(this).find('.cardlist:visible');  
            var $dataCols=thisCardlist.find('.cardlist-num').attr("data-cols");
            thisCardlist.equalHeight('.items-down-detail',$dataCols);
            thisCardlist.equalHeight('.items-down',$dataCols);
        });
    }
};
window.onload = function(){
    setflexHeight();
};

$('.service-v6-tabvideo [tab-title-list] li').click(function () {
    var thisIndex=$(this).index();
    $(this).addClass('active').siblings().removeClass('active');
    var thisTabody=$(this).closest('.tabs-head').next('.tabs-body');

    if(winWidth > 1023){
        thisTabody.find('.cardlist').eq(thisIndex).show(1,function () {
            setflexHeight();
        }).siblings().hide();
        /*thisTabody.find('.cardlist').eq(thisIndex).show().siblings().hide();

        setTimeout(function(){
            setflexHeight();
        },100);*/
      
    }else{
        thisTabody.find('.cardlist').eq(thisIndex).show().siblings().hide();
    }
});
$('.service-v6-tabvideo .tabs-head-mb').click(function(){
    $(this).toggleClass('active').next().slideToggle();
});
if(winWidth > 1023){
    $('.service-v6-tabvideo').each(function(){
        var $swiperContainer=$(this).find(".scroll-list-wrapper.swiper-container"),
            $uLeft=$(this).find(".u-icon-left"),
            $uRight=$(this).find(".u-icon-right");

        new Swiper($swiperContainer,{
            slidesPerView: 'auto',
            prevButton: $uLeft,
            nextButton: $uRight,
        });
    });
}
