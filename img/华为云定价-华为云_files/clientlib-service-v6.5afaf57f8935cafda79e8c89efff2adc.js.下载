/*! no static exports found */
/***/ (function(module, exports, __webpack_require__) {

var mediaQuery = __webpack_require__(/*! ./../../../../util/mediaQuery */ "./util/mediaQuery.js");

var initTimeline = function ($outer) {
    var $swiper = $outer.find('.swiper-container');
    if ($swiper.data('timeLine')) return;

    var $cards = $swiper.find('.service-v6-timeline-card');
    var el_prev = $outer.find('.service-v6-timeline-prev').get(0);
    var el_next = $outer.find('.service-v6-timeline-next').get(0);
    var maxQuantity = parseInt($swiper.attr('data-max-quantity') || 3);
    var cardQuantity = $cards.length;

    var slidesPerView = mediaQuery(979) ? (cardQuantity < maxQuantity ? cardQuantity : maxQuantity) : 1;

    var swiper = new Swiper($swiper.get(0), {
        slidesPerView: slidesPerView,
        prevButton: el_prev,
        nextButton: el_next,
    });

    $outer.data('timeLine', swiper);
};

$('.service-v6-timeline').each(function () {
    var $this = $(this);
    var $cards = $this.find('.service-v6-timeline-card');

    $cards.each(function () {
        var $this = $(this);
        var $date = $this.find('.service-v6-timeline-date');
        var datePastText = $.trim($date.attr('date-past')) || $.trim($date.text());
        var datePastArr = datePastText.split('.').map(function (item) {
            return parseInt(item);
        });
        // Date接受的月份为0-11对应1-12月
        var date = (new Date(datePastArr[0], datePastArr[1] - 1, datePastArr[2])).getTime();
        var today = date2ZeroTime(new Date);

        if (date < today) {
            $this.addClass('service-v6-timeline-card-past');
        }
    });

    if ($this.is(':visible')) {
        initTimeline($this);
    } else {
        var $tabContent = $this.closest('.v6-tab-content');
        $tabContent.on('afterShow', function () {
            initTimeline($this);
        });
    }
});

function date2ZeroTime(date) {
    date.setHours(0);
    date.setMinutes(0);
    date.setSeconds(0);
    date.setMilliseconds(0);

    return date.getTime();
}
    

