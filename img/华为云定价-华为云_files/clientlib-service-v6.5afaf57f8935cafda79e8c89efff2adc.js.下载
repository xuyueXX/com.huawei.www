                    if(_this_tool.find('.tab-content-list-item').eq(thisIndex).css('display') == 'block') {
                        this.timer = null;
                        setConveiToolHeight(_this_tool.find('.tab-content-list-item').eq(thisIndex).find('.product-v6-tabs'));
